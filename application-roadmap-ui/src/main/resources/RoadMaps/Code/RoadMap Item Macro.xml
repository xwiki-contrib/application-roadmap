    <property>
      <code>{{velocity}}
## retrieve all necessary parameters
#set ($users = $wikimacro.parameters.assignee)
#set ($description = $escapetool.xml($wikimacro.parameters.description))
#set ($optional = $escapetool.xml($wikimacro.parameters.optional))
#set ($project = $escapetool.xml($wikimacro.parameters.project))
#set ($urlString = $escapetool.xml($wikimacro.parameters.url))
#set ($statusKey = $xcontext.macro.params.status)
## get the config document and obj
#set ($confObj = $xwiki.getDocument("RoadMaps.Code.Configuration").get("RoadMaps.Code.ConfigurationClass"))
## get the json of the possible roadmap status values and their associated icons
#set ($json = $confObj.getStringValue("statusValues"))
#set ($jsonObj = $jsontool.fromString($json))
## get the id of the issue ex. https://jira.xwikisas.com/browse/XNNG-13 - we want the XNNG-13 
#set ($lastSlash = $stringtool.lastIndexOf($urlString, '/'))
#set ($file = $stringtool.substring($urlString, $lastSlash + 1))
## check if is github issue
#if ($stringtool.isNumeric($file))#set ($file = "#$file")#end
## use status name if icon is empty
#set ($icon = $jsonObj.get($statusKey).get("icon"))
#if ("$!icon" == '') #set($icon = $jsonObj.get($statusKey).get('prettyName'))#end
{{html wiki='true'}}
  &lt;span #if($optional=='true')class="optional"#end&gt;
    &lt;span class="status-icon" title="$!jsonObj.get($statusKey).prettyName"&gt;$services.icon.renderHTML($!icon)&lt;/span&gt;
    &lt;span class="users"&gt;
    #foreach ($user in $users)
      &lt;a href="$xwiki.getURL($user.reference)"&gt;{{useravatar username="$user.reference" width='20'
        height='20'/}} $user.reference.name&lt;/a&gt;#if($foreach.hasNext), #end
    #end
    &lt;/span&gt;: [[$file&gt;&gt;$urlString]]
    #if ("$!description" != '')
       - $description
    #end
    #if ("$!project" != '')
      &lt;em&gt; - $project&lt;/em&gt;
    #end
    #if ($optional=='true')
      &lt;em&gt;(optional)&lt;/em&gt;
    #end
  &lt;/span&gt;
{{/html}}
{{wikimacrocontent/}}
{{/velocity}}</code>
