    <property>
      <code>{{velocity}}&#xd;
## retrieve all necessary parameters&#xd;
#set ($users = $wikimacro.parameters.assignee)&#xd;
#set ($description = $escapetool.xml($wikimacro.parameters.description))&#xd;
#set ($optional = $escapetool.xml($wikimacro.parameters.optional))&#xd;
#set ($project = $escapetool.xml($wikimacro.parameters.project))&#xd;
#set ($urlString = $escapetool.xml($wikimacro.parameters.url))&#xd;
#set ($statusKey = $xcontext.macro.params.status)&#xd;
## get the config document and obj&#xd;
#set ($confObj = $xwiki.getDocument("RoadMaps.Code.Configuration").get("RoadMaps.Code.ConfigurationClass"))&#xd;
## get the json of the possible roadmap status values and their associated icons&#xd;
#set ($json = $confObj.getStringValue("statusValues"))&#xd;
#set ($jsonObj = $jsontool.fromString($json))&#xd;
## get the id of the issue ex. https://jira.xwikisas.com/browse/XNNG-13 - we want the XNNG-13 &#xd;
#set ($lastSlash = $stringtool.lastIndexOf($urlString, '/'))&#xd;
#set ($file = $stringtool.substring($urlString, $lastSlash + 1))&#xd;
## check if is github issue&#xd;
#if ($stringtool.isNumeric($file))#set ($file = "#$file")#end&#xd;
## use status name if icon is empty&#xd;
#set ($icon = $jsonObj.get($statusKey).get("icon"))&#xd;
#if ("$!icon" == '') #set($icon = 'list')#end&#xd;
{{html wiki='true'}}&#xd;
  &lt;span #if($optional=='true')class="optional"#end>&#xd;
    &lt;span class="status-icon" title="$!jsonObj.get($statusKey).prettyName">$!services.icon.renderHTML($!icon)&lt;/span>&#xd;
    &lt;span class="users">&#xd;
    #foreach ($user in $users)&#xd;
      &lt;a href="$xwiki.getURL($user.reference)">{{useravatar username="$user.reference" width='20'&#xd;
        height='20'/}} $user.reference.name&lt;/a>#if($foreach.hasNext), #else: #end&#xd;
    #end&#xd;
    &lt;/span>#if("$!urlString" != '')[[$file>>$urlString]] - #end&#xd;
    #if ("$!description" != '')&#xd;
      $description&#xd;
    #end&#xd;
    #if ("$!project" != '')&#xd;
      &lt;em> - $project&lt;/em>&#xd;
    #end&#xd;
    #if ($optional=='true')&#xd;
      &lt;em>(optional)&lt;/em>&#xd;
    #end&#xd;
  &lt;/span>&#xd;
{{/html}}&#xd;
{{wikimacrocontent/}}&#xd;
{{/velocity}}</code>
